   "source": [
    "query = \"\"\"\n",
    "SELECT COUNT(*) AS total_txns,\n",
    "       SUM(CASE WHEN Class = 1 THEN 1 ELSE 0 END) AS fraud_txns,\n",
    "       ROUND(SUM(Class)*100.0/COUNT(*), 2) AS fraud_rate\n",
    "FROM transactions;\n",
    "\"\"\"\n",
    "pd.read_sql_query(query, conn)"
   ]
